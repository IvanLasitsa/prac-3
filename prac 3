import random

number = str(random.randint(0, 101))

while True:
    guess = int(input("Введіть число "))
    
    if number > guess:
        print("Число більше ")
        
        
    elif guess > number:
        print("Число менше ")
        
    elif number == guess:
        print("Ви вгадали число ")
        exit()
        
